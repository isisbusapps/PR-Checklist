{{#each orgs}}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">{{name}}</h3>
        </div>
        <div class="panel-body">
            <ul>
                {{#each repos}}
                    <li>{{this}}</li>
                {{/each}}
            </ul>
        </div>
    </div>
{{/each}}